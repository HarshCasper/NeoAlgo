#include<iostream>
using namespace std;

void tower_of_hanoi(int n,char src,char dest,char helper){
      tower_of_hanoi(n-1,src,helper,dest);
      cout << "MOVE FROM " << src << " TO " << dest << endl;
      tower_of_hanoi(n-1,helper,dest,src);

      if(n==0){
      return ;  //base case
      }

}

int main(){
  tower_of_hanoi(3,'A','C','B');
  return 0;
 }
